{
  "name": "taiga-estimation-front",
  "version": "1.0.0",
  "description": "Frontend for Taiga Estimation, with Vue",
  "keywords": [
    "taiga",
    "estimation"
  ],
  "license": "MPL-2",
  "repository": {
    "type": "git",
    "url": "git@github.com:kaleidos-ventures/taiga-seedtime.git"
  },
  "homepage": "https://github.com/kaleidos-ventures/taiga-seedtime/tree/main#README",
  "bugs": {
    "url": "https://github.com/kaleidos-ventures/taiga-seedtime/tree/main#README"
  },
  "engines": {
    "node": ">= 16.19.1",
    "npm": ">= 8.19.3"
  },
  "scripts": {
    "dev": "npm run start",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "test": "cross-env NODE_ENV=test env-cmd src/.env npm run test:watch",
    "test:watch": "jest --watch --no-cache",
    "test:coverage": "jest --coverage",
    "clear_jest": "jest --clearCache",
    "test-debug": "node --inspect node_modules/.bin/jest --runInBand --watch --no-cache --o",
    "lint:js": "eslint --ext .js,.vue src",
    "lint:css": "stylelint src/**/*.vue src/**/*.css"
  },
  "jest": {
    "resetMocks": false,
    "resetModules": false,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.js",
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "axios": "^0.16.1",
    "custom-env": "^1.0.0",
    "env-cmd": "^8.0.2",
    "murmurhash-js": "1.0.0",
    "sha1": "^1.1.1",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^2.8.1",
    "vue-server-renderer": "^2.6.10",
    "vue-translate-plugin": "^1.2.0",
    "vuedraggable": "^2.19.3",
    "vuex": "^2.5.0"
  },
  "devDependencies": {
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.28",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "flush-promises": "^1.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.differenceby": "^4.8.0",
    "lodash.pick": "^4.4.0",
    "lodash.shuffle": "^4.2.0",
    "lolex": "^1.5.2",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.4.0",
    "phantomjs-prebuilt": "^2.1.16",
    "postcss": "^5.2.18",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.3",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.2",
    "semver": "^5.6.0",
    "shelljs": "^0.7.6",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.4.3",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^1.1.2",
    "vue-eslint-parser": "^5.0.0",
    "vue-jest": "^3.0.2",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^2.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuex-mock-store": "0.0.7",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4"
  },
  "pre-commit": [
    "lint:css",
    "lint:js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "overrides": {
    "eslint-plugin-vue": {
      "eslint": "^3.19.0"
    },
    "url-loader": {
      "webpack": "^2.3.3"
    },
    "vue-eslint-parser": {
      "eslint": "^3.19.0"
    }
  }
}
